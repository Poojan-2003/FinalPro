<body>
    <div class="Container">
            <div class="Navbar">
                   <img id="img" src="Fpage.png">
                    <!-- <input type="text" id="search" placeholder="Search"> -->
                    <span class="auto-type" id="search"></span>
                    <div id="about_us" ><a id="search1" href="about_us.html">About Us</a></div>
            </div>
<div class="body2">
        <table>
            <tr>
                <th id="sr">Sr No</th>
                <th id="id">ID NO</th>
                <th id="name">Name</th>
                <th id="area">Project Defination</th>
                <th id="area">Project Link</th>
                <th id="topic">Technology used in Project</th>
                <th id="know">Know More</th>
              </tr>
<tr>
                 <% var i =0 %>
                 <% employeelist.forEach(datadetail =>{ %>
               <tr>
                <% ++i %>
                <th><%= i %></th>
                <th><%= datadetail.id %></th>
                <th><%= datadetail.Name %></th>
                <th><%= datadetail.speciality %></th>
                <th><%= datadetail.devlopment %></th>
                <th><%= datadetail.topics%></th>
                
              <th> <button id="btn"><a id="a" href="completeinfo">Know More</button></th>
            </tr>
                <%})%>
</tr>

        </table>
    
    </div>

</div>

<script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>
<script>var typed = new Typed(" .auto-type",{
    strings : ["Welcome To Informer" , "Explore Different Projects"],
    typeSpeed : 100,
    backSpeed : 100,
    loop : true
})
</script>
</body>
</html>